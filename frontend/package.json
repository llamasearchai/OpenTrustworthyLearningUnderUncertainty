  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "size-limit": [
    {
      "path": "dist/**/*.js",
      "limit": "300 KB"
    },
    {
      "path": "dist/assets/vendor*.js",
      "limit": "75 KB"
    }
  ],
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@8.15.0"
}
